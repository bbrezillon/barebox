		pinctrl_disp0: disp0grp {
			fsl,pins = <
				MX53_PAD_DI0_DISP_CLK__IPU_DI0_DISP_CLK	0x000004c1
				MX53_PAD_DI0_PIN15__IPU_DI0_PIN15	0x000004c1
