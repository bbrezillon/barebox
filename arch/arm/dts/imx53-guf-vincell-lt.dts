				MX53_PAD_EIM_A23__EMI_WEIM_A_23 	0x000001e4
				MX53_PAD_EIM_A22__EMI_WEIM_A_22 	0x000000e4
				MX53_PAD_EIM_A21__EMI_WEIM_A_21 	0x000000e4
